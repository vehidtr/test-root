{
  "version": 2,
  "middleware": [
    {
      "source": "/app/:path*",
      "destination": "https://app.moonastudios.com/:path*"
    },
    {
      "source": "/:path*",
      "destination": "https://landing.moonastudios.com/:path*"
    }
  ],
  "rewrites": [
    {
      "source": "/app/:path*",
      "destination": "https://app.moonastudios.com/:path*"
    },
    {
      "source": "/:path*",
      "destination": "https://landing.moonastudios.com/:path*"
    }
  ]
}
